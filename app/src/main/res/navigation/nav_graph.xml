<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/nav_graph_home">

    <fragment
        android:id="@+id/nav_graph_home"
        android:label="Home"
        android:name="com.example.keyboardexperiments.ui.HomeFragment"
        tools:layout="@layout/fragment_home" />

    <dialog
        android:id="@+id/nav_graph_xml_bottom_sheet"
        android:label="XML Bottom Sheet"
        android:name="com.example.keyboardexperiments.ui.XmlBottomSheet"
        tools:layout="@layout/fragment_xml_bottom_sheet" />

    <dialog
        android:id="@+id/nav_graph_compose_bottom_sheet"
        android:label="Compose Bottom Sheet"
        android:name="com.example.keyboardexperiments.ui.ComposeBottomSheet"
        tools:layout="@layout/fragment_compose_bottom_sheet" />

    <dialog
        android:id="@+id/nav_graph_bottom_sheet_nav_graph"
        android:label="Bottom Sheet Nav Graph"
        android:name="com.example.keyboardexperiments.ui.NavGraphBottomSheet"
        tools:layout="@layout/fragment_nav_graph_bottom_sheet" />

</navigation>